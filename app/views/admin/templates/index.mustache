<h2>Layouts</h2> 
<ul>
  {{#layouts}} 
    <li class="layout" id="layout-{{id}}"> 
      <div class="item"> 
        <span class="title"> 
           <a id id="edit-layout-{{id}}" href="#/templates/{{id}}/edit">{{name}}</a>       
        </span> 
        <span class="modify">                 
          <a class="remove-layout" id="remove-layout-{{id}}" href="#/layouts/{{id}}/remove">x</a> 
        </span>  
      </div>
    </li>  
  {{/layouts}}  
</ul>   

